{
  "name": "log-ingestor-system",
  "version": "1.0.0",
  "description": "A log ingestor system that can efficiently handle vast volumes of log data, and offer a simple interface for querying this data using full-text search or specific field filters.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run start\" \"npm run client\""
  },
  "keywords": [
    "log",
    "ingestor",
    "query",
    "interface",
    "full-text",
    "search",
    "filter"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "mongoose": "^5.12.3",
    "concurrently": "^6.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
